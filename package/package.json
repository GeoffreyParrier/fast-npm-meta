{
  "name": "npm-meta-info",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@9.4.0",
  "description": "Get npm package metadata",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/npm-meta-info#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/npm-meta-info.git"
  },
  "bugs": "https://github.com/antfu/npm-meta-info/issues",
  "keywords": [],
